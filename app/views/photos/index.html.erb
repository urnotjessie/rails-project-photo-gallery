<% @photos.each do |photo| %>
  <%= image_tag photo.thumbnail %>
  <%= photo.caption %>
  <%= link_to "#{photo.user.username}", user_path(photo.user) %>

  <% if session[:user_id] %>
    <%= link_to "Add to my collection", new_photo_photo_user_path(photo.id) %>
  <% end %>

<% end %>

<br>

<% if session[:user_id] %>
  <%= link_to 'Log out', logout_path, :method => 'post' %>
<% end %>
